{
  "expo": {
    "name": "HolyQuraan",
    "slug": "HolyQuraan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/intro.png",
    "scheme": "holyquraan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/intro.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/intro.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.heshamsaleh.HolyQuraan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "64103d46-7264-49af-af9f-54e5311c0691"
      }
    }
  }
}
